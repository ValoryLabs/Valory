{
  "name": "valory-v2",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0",
    "bun": ">=1.2.0"
  },
  "scripts": {
    "dev": "concurrently -k -n \"üé®Frontend,üöÄBackend,üåêCaddy\" -c \"cyan,green,yellow\" \"bun run dev:frontend\" \"bun run dev:backend\" \"bun run dev:caddy\"",
    "dev:frontend": "bun run codegen && vite",
    "dev:backend": "bash -c 'cd ../backend && source .venv/bin/activate && uvicorn main:app --reload'",
    "dev:caddy": "caddy run --config ../Caddyfile.dev",
    "dev:full": "bun run codegen:watch & bun run dev",
    "build": "vite build",
    "build:analyze": "vite build --mode analyze",
    "preview": "vite preview",
    "start": "vite preview",
    "codegen": "bun run src/api/codegen-api.ts",
    "codegen:watch": "nodemon --watch '../backend' --ext 'py' --exec 'bun run codegen'",
    "type-check": "vue-tsc --build",
    "lint": "biome check .",
    "lint:fix": "biome check --write .",
    "format": "biome format --write .",
    "test": "echo \"No tests yet\" && exit 0"
  },
  "dependencies": {
    "@basitcodeenv/vue3-device-detect": "^1.0.3",
    "@unhead/vue": "^2.0.0-alpha.16",
    "@vueuse/core": "^13.3.0",
    "@vueuse/motion": "^3.0.3",
    "@vueuse/router": "^13.0.0",
    "axios": "^1.11.0",
    "canvas-confetti": "^1.9.3",
    "cheerio": "^1.1.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-vue-next": "^0.487.0",
    "motion-v": "^1.7.1",
    "ofetch": "^1.4.1",
    "ogl": "^1.0.11",
    "pinia": "^3.0.1",
    "pinia-plugin-persistedstate": "^4.2.0",
    "reka-ui": "^2.3.1",
    "tailwind-merge": "^3.2.0",
    "tailwindcss-animate": "^1.0.7",
    "tinycolor2": "^1.6.0",
    "vaul-vue": "^0.2.1",
    "vue": "^3.5.13",
    "vue-i18n": "npm:petite-vue-i18n@^10.0.0",
    "vue-router": "^4.5.0",
    "vue-sonner": "^1.3.0"
  },
  "devDependencies": {
    "@types/axios": "^0.14.4",
    "@types/cheerio": "^1.0.0",
    "@vitejs/plugin-vue": "^5.2.1",
    "vite": "^6.0.11",
    "vue-tsc": "^2.2.0",
    "typescript": "~5.7.3",
    "@tailwindcss/vite": "^4.1.3",
    "tailwindcss": "^4.1.3",
    "@biomejs/biome": "^2.2.3",
    "swagger-typescript-api": "^13.2.9",
    "concurrently": "^9.2.1",
    "nodemon": "^3.1.10",
    "@types/node": "^24.3.1",
    "@types/tinycolor2": "^1.4.6",
    "@tsconfig/node22": "^22.0.0",
    "@vue/tsconfig": "^0.7.0",
    "@intlify/unplugin-vue-i18n": "^6.0.3",
    "@number-flow/vue": "^0.4.7",
    "rollup-plugin-visualizer": "^5.12.0",
    "vite-plugin-minify": "^2.1.0",
    "sass-embedded": "^1.85.0",
    "jiti": "^2.4.2",
    "tw-animate-css": "^1.2.5"
  },
  "trustedDependencies": [
    "@tailwindcss/oxide"
  ]
}
