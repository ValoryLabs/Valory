{
  "name": "monorepo-valory",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,vue,js,jsx,json,md}\""
  },
  "devDependencies": {
    "eslint-config-turbo": "^2.5.6",
    "prettier": "latest",
    "turbo": "latest"
  },
  "packageManager": "bun@1.2.0"
}
